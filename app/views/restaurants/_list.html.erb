<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>phone_number</th>
      <th>category</th>
    </tr>
  </thead>
  <tbody>
    <% restos.each do |restaurant| %>
      <tr>
        <td><%= restaurant.name %></td>
        <td><%= restaurant.address %>
          <td><%= restaurant.phone_number %></td>
          <td><%= restaurant.category %> </td>
          <td><%= link_to 'Show', restaurant %></td>
        </tr>
      <% end %>
    </tbody>
  </table>